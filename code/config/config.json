{
  "data_distribution": {
    "dataset_name": "cifar10",
    "test_data_ratio": 0.2,
    "clients_number": 2,
    "main_classes_per_client_number": 5,
    "main_class_ownership_per_client_ratio": 0.6
  },
  "learning": {
    "cycle": {
      "iterations": 5,
      "iterations_to_aggregate": 1
    },
    "client": {
      "weights_sending": {
        "send_only_changed_weights": false,
        "minimum_weight_difference_to_send": 1
      }
    },
    "server": {
      "early_stopping": {
        "enabled": false,
        "metric_type": "accuracy",
        "patience": 2
      }
    }
  }
}
